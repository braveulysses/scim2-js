{
  "name": "scim2-js",
  "version": "0.1.0",
  "description": "SCIM 2 for JavaScript",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "keywords": [
    "identity",
    "scim",
    "scim2"
  ],
  "author": "Jacob Childress <jacobc@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "6",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "6",
    "jest": "^18.1.0"
  },
  "dependencies": {
    "object-path": "^0.11.3",
    "strscan": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|docs|node_modules)[/\\\\]"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js)$"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
